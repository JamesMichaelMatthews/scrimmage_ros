<launch>
  <arg name="mission_file" default="uuv-ex1"/>
  <arg name="entity_name" default="uuv_entity"/>
  <arg name="entity_id" default="1"/>
  <arg name="max_contacts" default="100"/>

  <param name="my_param" type="string" value="hi"/>
  
  <node pkg="nodelet" type="nodelet" name="standalone_nodelet"  args="manager" output="screen"/>
  <node pkg="nodelet" type="nodelet" name="scrimmage_nodele" args="load scrimmage_ros/Nodelet standalone_nodelet" output="screen">
    <param name="mission_file" value="$(arg mission_file)" type="str"/>
    <param name="entity_name" value="$(arg entity_name)" type="str"/>
    <param name="entity_id" value="$(arg entity_id)" type="int"/>
    <param name="max_contacts" value="$(arg max_contacts)" type="int"/>
    <param name="loop_rate_hz" type="double" value="2"/>
  </node>
</launch>
